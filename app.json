        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gid3onjura.expoexampleapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "04979101-3dbe-4a11-ad8f-2aab95db3092"
      }
    }
  }
}
